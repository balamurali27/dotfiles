! #include "/home/balu/.Xresources.d/custom-themes/narutov2.Xresources"
! #include "/home/balu/.Xresources.d/Xresources-themes-master/xcolors-zenburn.Xresources"
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
! open new terminal in current directory
! windows key is Mod4, enter is Return
URxvt.keysym.Shift-Mod4-Return: urxvtc\n
! *.font:-*-terminus-medium-r-normal--16-*-*-*-*-*-*-*
! URxvt*font: xft:DejaVu Sans Mono:size=10:antialias=true
URxvt.secondaryScroll: true
URxvt.secondaryWheel: true

!*.font: xft:Iosevka Term:style=Regular:size=12:antialias=true,hinting=true
!*.boldFont: xft:Iosevka Term:style=Bold:size=12
!*.italicFont: xft:Iosevka Term:style=Italic:size=12
!*.boldItalicFont: xft:Iosevka Term:style=Bold Italic:size=12

*.font: xft:Anka/Coder:style=Regular:size=13:antialias=true,hinting=true

! default window size
URxvt.geometry: 60x20
URxvt*perl-ext-common: eval,selection,resize-font,matcher
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1
URxvt*scrollBar: false
URxvt*highlightSelection: true
! for true transparency
 URxvt*depth: 32
URxvt*background: rgba:0000/0000/0200/c800
! URxvt*shading: 99 to 0 darkens, 101 to 200 lightens
URxvt*shading: 10
!*transparent: true
! URxvt*blurRadius: 5
URxvt.keysym.Home: \033[7~
URxvt.keysym.End: \033[8~
URxvt.keysym.KP_Home: \033[7~
URxvt.keysym.KP_End:  \033[8~
! for the weird ISO popup at bottom left to stop
URxvt.iso14755: false
URxvt.iso14755_52: false
! do not scroll with output
URxvt*scrollTtyOutput: false
! scroll in relation to buffer (with mouse scroll or Shift+Page Up)
URxvt*scrollWithBuffer: true
! scroll back to the bottom on keypress
URxvt*scrollTtyKeypress: true
