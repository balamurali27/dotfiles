#include "/home/balu/.Xresources.d/custom-themes/seoul256.Xresources"
/* #include "/home/balu/.Xresources.d/Xresources-themes-master/iterm-Molokai.Xresources" */
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
! open new terminal in current directory
! windows key is Mod4, enter is Return
URxvt.keysym.Shift-Mod4-Return: urxvtc\n
!*.font:-*-terminus-medium-r-normal--16-*-*-*-*-*-*-*
!URxvt*font: xft:DejaVu Sans Mono:size=10:antialias=true

!does not work
!URxvt*font: xft:DejaVu Sans:size=10:antialias=true, xft:Noto Emoji:minspace=False

URxvt.secondaryScroll: true
URxvt.secondaryWheel: true

*.font: xft:Iosevka Term:style=Regular:size=12:antialias=true
*.boldFont: xft:Iosevka Term:style=Bold:size=12:antialias=true
*.italicFont: xft:Iosevka Term:style=Italic:size=12:antialias=true
*.boldItalicFont: xft:Iosevka Term:style=Bold Italic:size=12:antialias=true

!*.font: xft:Anka/Coder:style=Regular:size=13:antialias=true,hinting=true

! default window size
!URxvt.geometry: 60x20
URxvt*perl-ext-common: eval,selection,resize-font,matcher
URxvt.url-launcher: /usr/bin/vendor_perl/mimeopen
URxvt.matcher.button: 1
URxvt*scrollBar: false
URxvt*highlightSelection: true
! for true transparency
! URxvt*depth: 32
! URxvt*background: rgba:0000/0000/0200/c800 
! URxvt*shading: 99 to 0 darkens, 101 to 200 lightens
!URxvt*shading: 70
!*transparent: true
! URxvt*blurRadius: 35
URxvt.keysym.Home: \033[7~
URxvt.keysym.End: \033[8~
URxvt.keysym.KP_Home: \033[7~
URxvt.keysym.KP_End:  \033[8~
! for the weird ISO popup at bottom left to stop
URxvt.iso14755: false
URxvt.iso14755_52: false
! do not scroll with output
URxvt*scrollTtyOutput: false
! scroll in relation to buffer (with mouse scroll or Shift+Page Up)
URxvt*scrollWithBuffer: true
! scroll back to the bottom on keypress
URxvt*scrollTtyKeypress: true
! send same escape sequences as xterm
URxvt.keysym.Control-Up:     \033[1;5A
URxvt.keysym.Control-Down:   \033[1;5B
URxvt.keysym.Control-Left:   \033[1;5D
URxvt.keysym.Control-Right:  \033[1;5C
!lovelace colors
*background:	#1D1F28
*foreground:	#FDFDFD
*cursorColor:	#C574DD
*color0:	#282A36
*color1:	#F37F97
